		id=":as0"
		static="true"
		margin="2"
		startAngle="135"
		sweepAngle="270"
		strokeWidth="10" />
	<arc-stroke
		id=":as1"
		static="false"
		margin="2"
		startAngle="132"
		sweepAngle="276"
		strokeWidth="12" />
	<arc-stroke
		id=":as2"
		static="false"
		margin="2"
		startAngle="135"
		sweepAngle="270"
		strokeWidth="8" />
	<text-board
		id=":tp"
		drawRect="%,0.5,0.5,0.5,0.5"
		static="false"
		textSize="10"
		align="center"
		font="assets:///r2014.ttf"
		text="50%" />
	<text-board
		id=":tt"
		drawRect="%,0,0.8,1,0.9"
		textSize="10"
		align="center" />
