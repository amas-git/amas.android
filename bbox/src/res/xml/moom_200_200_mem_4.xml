<?xml version="1.0" encoding="utf-8"?>
<!-- 
	彩虹 -->
<moom
	name="太君"
	tag=":memory"
	bg="@drawable/white"
	zoom="0.65"
	bg-margin="0.1"
	size="200x200">
	<text-board
		drawRect="0,-2,200,-2"
		textSize="20"
		alpha="255"
		color="black"
		text="Only so much" />
	<image
		margin="1"
		src="@drawable/sb" />
	<text-board
		drawRect="%,0.5,0,1,0.3"
		expr="%MEM_USED"
		static="false"
		textSize="40"
		alpha="255"
		color="black"
		text="90%" />
	<text-board
		expr="%MEM_FREE"
		drawRect="%,0,0.15,0.95,0.95"
		static="false"
		textSize="20"
		alpha="255"
		color="black"
		text="90%" />
</moom>